    "# Define at most five keywords for the product\n",
    "product_keywords = [ \n",
    "    \"\",\n",
    "    \"\"\n",
    "] \n",
    "\n",
    "# Define spatial  in epsg:4326. If the dataset covers discontinuous regions, \\\n",
